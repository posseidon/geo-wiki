<html>
<head>
  <title>@ELTE Térképész - Geographical Data</title>
  <!-- Blueprint CSS Framework -->
  <%= stylesheet_link_tag 'blueprint/screen' %>
  <%= stylesheet_link_tag 'blueprint/plugins/fancy-type/screen' %>
  <!-- JQuery Fancybox css -->
  <%= stylesheet_link_tag 'fancybox/jquery.fancybox-1.3.1' %>
  <%= stylesheet_link_tag 'smoothness/jquery-ui-1.8.5.custom' %>
  <!-- MB Slider -->
  <%= stylesheet_link_tag 'mb/css/mbExtruder' %>
  <!-- Own Stylesheet -->
  <%= stylesheet_link_tag 'articles/biography'%>
  <%= stylesheet_link_tag 'smoothness/jquery-ui'%>
  <%= javascript_include_tag :defaults %><%= yield :javascripts %>
  <!-- JQuery related Includes -->
  <%= javascript_include_tag 'jquery.jeditable'%>
  <%= javascript_include_tag 'fancybox/jquery.fancybox-1.3.1'%>
  <%= javascript_include_tag 'fancybox/jquery.easing-1.3.pack'%>
  <%= javascript_include_tag 'ckeditor/ckeditor'%>
  <%= javascript_include_tag 'ckeditor/adapters/jquery'%>
  <!-- Additional Javascripts: MarkerClusterer for Google maps -->
  <%= javascript_include_tag 'articles/markerclusterer' %>



  <script type="text/javascript" src="http://maps.google.com/maps?file=api&amp;v=3&amp;key=ABQIAAAAuLaPZSP2TB-U002ZA3ZhKhTJQa0g3IQ9GZqIMmInSLzwtGDKaBTNqQWhnKZu-e6YbeqTBsNDicWcYA"></script>
  <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
  <script type="text/javascript">
	  function initialize() {
		  if (GBrowserIsCompatible()) {
			initializep();
		  }
	  }
  </script>
</head>

<body onload="initialize()" onunload="GUnload()">
  <%= yield %>
</body>
</html>
